	<div class="bg0 m-t-70 p-b-140">
		<div class="container  pt-5 mt-5">
			<div class="flex-w flex-sb-m p-b-52">
				<div class="flex-w flex-l-m filter-tope-group m-tb-10">
					<a href="/productView" class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
						All Products
					</a>
					<% if (category && category.length> 0) { %>
						<% category.forEach(singleCategory=> { %>
							<a href="/productView?category=<%= encodeURIComponent(singleCategory.categoryName) %>&sort=<%= currentSort || '' %>&search=<%= search || '' %>" class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
								<%= singleCategory.categoryName %>
							</a>
							<% }); %>
								<% } %>

				</div>
				<div class="flex-w flex-c-m m-tb-10">
					<div
						class="flex-c-m stext-106 cl6 size-104 bor4 pointer hov-btn3 trans-04 m-r-8 m-tb-4 js-show-filter">
